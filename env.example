# Pet-Portal Environment Variables
# Copy this file to .env and fill in your actual values

# Firebase Configuration
PROJECT_ID=pet-portal-mvp

# Sharetribe Flex API Keys
SHARETRIBE_CLIENT_ID=your_sharetribe_client_id
SHARETRIBE_CLIENT_SECRET=your_sharetribe_client_secret_here

# Toss Payments API Keys
TOSS_CLIENT_KEY=your_toss_client_key_here
TOSS_SECRET_KEY=your_toss_secret_key_here
TOSS_VERIFY_KEY=your_toss_verify_key_here

# Kakao API (Optional)
KAKAO_REST_API_KEY=your_kakao_rest_api_key_here

# Firebase Admin SDK (for server-side)
FIREBASE_PROJECT_ID=pet-portal-mvp
FIREBASE_PRIVATE_KEY_ID=your_firebase_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@pet-portal-mvp.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your_firebase_client_id
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token
FIREBASE_AUTH_PROVIDER_X509_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
FIREBASE_CLIENT_X509_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/firebase-adminsdk-xxxxx%40pet-portal-mvp.iam.gserviceaccount.com 